<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_click_my_help"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dcch.sharebike.moudle.login.activity.ClickMyHelpActivity">

    <ImageView
        android:id="@+id/close_view_pager"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:src="@mipmap/hintcard_close" />


    <android.support.v4.view.ViewPager
        android:layout_below="@+id/close_view_pager"
        android:id="@+id/explainPage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </android.support.v4.view.ViewPager>

    <LinearLayout
        android:dividerPadding="10dp"
        android:id="@+id/ll_pager_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/explainPage"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-100dp"
        android:orientation="horizontal">

    </LinearLayout>


</RelativeLayout>
